/*Find if the number is palindrome or not*/
#include<stdio.h>
int main()
{
    int num,a,d,sum=0;
    printf("Enter a number : ");
    scanf("%d",&a);
    num=a;
    while(num!=0)
    {
        d=num%10;
        sum=sum*10+d;
        num=num/10;
    }
    if(a==sum)
    {
        printf("It's a Palindrome number");
    }
    else {
        printf("It's not a Palindrome number");
    }
    return 0;
}
